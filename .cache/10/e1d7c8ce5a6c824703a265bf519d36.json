s;\n  }\n  `;\n};\nclass RainbowText extends HTMLElement {\n  addSpanEventListeners(span) {\n    span.addEventListener(\"mouseover\", () => span.classList.add(\"hovered\"));\n    span.addEventListener(\"animationend\", () =>\n      span.classList.remove(\"hovered\")\n    );\n  }\n  addSpans(div) {\n    [this.text].forEach(letter => {\n      const span = this.createSpan(letter);\n      div.appendChild(span);\n    });\n  }\n  addStyle() {\n    const styleTag = document.createElement(\"style\");\n    styleTag.textContent = getStyle(this.size);\n    this.shadowRoot.appendChild(styleTag);\n  }\n  connectedCallback() {\n    this.createShadowRoot();\n    this.text = this.getAttribute(\"text\");\n    this.size = this.getAttribute(\"font-size\");\n    this.render();\n  }\n  createSpan(letter) {\n    const span = document.createElement(\"span\");\n    span.classList.add(\"letter\");\n    span.textContent = letter;\n    this.addSpanEventListeners(span);\n    return span;\n  }\n  render() {\n    const div = document.createElement(\"div\");\n    div.classList.add(\"header\");\n    this.shadowRoot.appendChild(div);\n    this.addSpans(div);\n    this.addStyle();\n  }\n}\n\ntry {\n  customElements.define(\"rainbow-text\", RainbowText);\n} catch (err) {\n  const h3 = document.createElement(\"h3\");\n  h3.innerHTML =\n    \"This site uses webcomponents which don't work in all browsers! Try this site in a browser that supports them!\";\n  document.body.appendChild(h3);\n}\n"},"lineCount":null}},"error":null,"hash":"5ac09915255a61b828b67f06c9b499a1","cacheData":{"env":{}}}